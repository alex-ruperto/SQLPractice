-- Creating a staging table for imports
CREATE TABLE staging_import (
    date DATE,
    open NUMERIC,
    high NUMERIC,
    low NUMERIC,
    close NUMERIC,
    adj_close NUMERIC,
    volume NUMERIC
);

-- Creating the main stocks table
CREATE TABLE stocks (
    ticker_symbol VARCHAR,
    date VARCHAR,
    open FLOAT4,
    high FLOAT4,
    low FLOAT4,
    close FLOAT4,
    adj_close FLOAT4,
    volume INT4
);